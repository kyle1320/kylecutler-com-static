{
  "name": "kylecutler-com-static",
  "version": "0.0.1",
  "dependencies": {
    "jscolor-picker": "^2.0.4",
    "three": "^0.141.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.6.0",
    "@parcel/transformer-glsl": "^2.6.0",
    "@parcel/transformer-pug": "^2.6.0",
    "@parcel/transformer-sass": "^2.6.0",
    "@parcel/transformer-webmanifest": "^2.6.0",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "parcel": "^2.6.0",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "dev": "parcel \"src/content/**/*.pug\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx",
    "verify": "npm run typecheck && npm run lint",
    "build": "parcel build \"src/content/**/*.pug\""
  },
  "alias": {
    "react/jsx-runtime": "./src/common/js/jsx.ts",
    "react/jsx-dev-runtime": "./src/common/js/jsx.ts"
  }
}
